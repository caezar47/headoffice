{% if view == "services" %}
	{% for value in list %}	
	<a href="{{value.link}}" class="{{prefix}}item  {{mod}}">
		<picture class="{{prefix}}preview  {{mod}}">
			<img src="{{value.img}}" class="img-responsive" alt="">
		</picture>
		<p class="{{prefix}}heading  {{mod}}">{{value.name}}</p>
	</a>
	{% endfor %}
{% endif %}
{% if view == "team-full" %}
	{% for value in list %}	
	<div class="{{prefix}}item  {{mod}}">
		<picture class="{{prefix}}preview  {{mod}}">
			<source media="(max-width: 575px)" srcset="{{value.img_xs}}">
			<img src="{{value.img}}" class="img-responsive" alt="">
		</picture>
		<div>
			<p class="{{prefix}}heading  {{mod}}">{{value.name}}</p>
			<p class="{{prefix}}position  {{mod}}">{{value.position}}</p>
			<div class="{{prefix}}note  {{mod}}">{{value.note}}</div>
			<div class="{{prefix}}btn  {{mod}}">
				{% include "../button/btn.twig" with {
					view:"link",
					prefix:"btn__", 
					mod:"is--icon", 
					icon:'icon-next', 
					attr: '',
					link:value.link, 
					name:"Подробнее" 
				} %}
			</div>
		</div>
	</div>
	{% endfor %}
{% endif %}
