					<p class="{{prefix}}position  {{mod}}">{{value.position}}</p>
					<picture class="{{prefix}}preview  {{mod}}-sign">
						<img src="{{value.sign}}" class="img-responsive" alt="">
					</picture>
					{# <p class="{{prefix}}btn  {{mod}}">
						{% include "../button/btn.twig" with {
							view:"link",
							prefix:"btn__", 
							mod:"is--icon", 
							icon:'icon-next', 
							attr: '',
