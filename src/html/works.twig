        {#<script>
            $("[data-tabs-next]").on("click", function(e) {
                var $active = $(this).closest(".card__note").find(".tabs__pane.active");
                var $next = $active.next();
                console.log($active);
                var $active_tabs = $(this).closest(".card__note").find(".tabs__nav.is--step .active");
                var $next_tabs = $active_tabs.next();
                var $step = $next_tabs.find("a").data("step");


                if ($next.length) {
                    $active.removeClass("active");
                    $next.addClass("active");
                    $active_tabs.removeClass("active");
                    $active_tabs.addClass("visited");
                    $next_tabs.addClass("active");
                    $(this).closest(".card__note").find("[data-tabs-count]").text($step); 
                }
            });
            $('.card__group.is--works').on('shown.bs.collapse', function () {
                var all = $(this).find('.collapse.in .tabs__item.is--step').length
                $(this).find('.collapse.in [data-tabs-count-all]').text(all);
            });
            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                var href = $(e.target).attr('href');
                var $curr = $(this).closest(".card__note").find(".tabs__nav.is--step a[href='" + href + "']").parent();
                var $currStep = $(this).closest(".card__note").find(".tabs__nav.is--step a[href='" + href + "']").data("step");

                $(this).closest(".card__note").find('.tabs__item.is--step').removeClass("active");
                $(this).closest(".card__note").find('.tabs__item.is--step').removeClass("visited");

